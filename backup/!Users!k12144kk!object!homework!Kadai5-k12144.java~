class Kadai5 {
	public static void main(String[] args) throws IOException
	{
		/* BufferedReaderï¿½ÌƒCï¿½ï¿½ï¿½Xï¿½^ï¿½ï¿½ï¿½Xï¿½ï¿½ */
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));


		/* Sortï¿½Nï¿½ï¿½ï¿½Xï¿½ï¿½sort1ï¿½Æ‚ï¿½ï¿½ÄƒCï¿½ï¿½ï¿½Xï¿½^ï¿½ï¿½ï¿½Xï¿½ï¿½ */
		Sort sort1 = new Sort();

		/* ï¿½Û‘ï¿½4_1ï¿½É‘Î‰ï¿½ï¿½ï¿½ï¿½é•”ï¿½ï¿½ï¿½Jï¿½n */
		System.out.println("ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½é—ï¿½ï¿½ï¿½ÌŒÂï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Í‚ï¿½ï¿½Ä‚ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½");
		int n_max = Integer.parseInt(br.readLine());

		System.out.println("ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½é—ï¿½ï¿½ï¿½ÌÅï¿½ï¿½lï¿½ï¿½ï¿½ï¿½ï¿½Í‚ï¿½ï¿½Ä‚ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½");
		int r_min = Integer.parseInt(br.readLine());

		System.out.println("ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½é—ï¿½ï¿½ï¿½ÌÅ‘ï¿½ï¿½lï¿½ï¿½ï¿½ï¿½ï¿½Í‚ï¿½ï¿½Ä‚ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½");
		int r_max = Integer.parseInt(br.readLine());


		/* sort1ï¿½ï¿½makeRndï¿½ï¿½ï¿½Äo */
		sort1.makeRnd(n_max, r_min, r_max);

		System.out.println("ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½lï¿½ï¿½,");
		sort1.show();
		System.out.println("ï¿½Å‚ï¿½.");

		/* sort1ï¿½ï¿½bubbleGreaterï¿½ï¿½ï¿½Äo */
		sort1.bubbleGreater();
		System.out.println("ï¿½ï¿½ï¿½ï¿½ï¿½Éƒoï¿½uï¿½ï¿½ï¿½\ï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Ê‚ï¿½,");
		sort1.show();
		System.out.println("ï¿½Å‚ï¿½.");

		/* ï¿½ï¿½ï¿½Úƒtï¿½Bï¿½[ï¿½ï¿½ï¿½hï¿½ÉƒAï¿½Nï¿½Zï¿½X */
		System.out.println("ï¿½Åï¿½ï¿½l:" + sort1.num[0] + ", " +
					n_max/2 + "ï¿½Ô–Ú‚Ì’l:" + sort1.num[n_max/2-1] +
					", ï¿½Å‘ï¿½ï¿½l:" + sort1.num[sort1.num.length-1] + "ï¿½Å‚ï¿½.");



		/* Sortï¿½Nï¿½ï¿½ï¿½Xï¿½ï¿½sort2ï¿½Æ‚ï¿½ï¿½ÄƒCï¿½ï¿½ï¿½Xï¿½^ï¿½ï¿½ï¿½Xï¿½ï¿½ */
		Sort sort2 = new Sort();

		/* ï¿½Û‘ï¿½4_2ï¿½É‘Î‰ï¿½ï¿½ï¿½ï¿½é•”ï¿½ï¿½ï¿½Jï¿½n */
		System.out.println("ï¿½Aï¿½ï¿½ï¿½lï¿½ÌÅï¿½ï¿½lï¿½ï¿½ï¿½ï¿½ï¿½Í‚ï¿½ï¿½Ä‚ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½");
		r_min = Integer.parseInt(br.readLine());

		System.out.println("ï¿½Aï¿½ï¿½ï¿½lï¿½ÌÅ‘ï¿½ï¿½lï¿½ï¿½ï¿½ï¿½ï¿½Í‚ï¿½ï¿½Ä‚ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½");
		r_max = Integer.parseInt(br.readLine());

		/* sort2ï¿½ï¿½makeSequenceï¿½ï¿½ï¿½Äo */
		sort2.makeSequence(r_min, r_max);
		System.out.println("ï¿½zï¿½ï¿½ï¿½ï¿½");
		sort2.show();
		System.out.println("ï¿½ï¿½ï¿½pï¿½Ó‚ï¿½ï¿½Ü‚ï¿½ï¿½ï¿½.");

		/* sort2ï¿½ï¿½shuffleï¿½ï¿½ï¿½Äo */
		sort2.shuffle();
		System.out.println("ï¿½Vï¿½ï¿½ï¿½bï¿½tï¿½ï¿½ï¿½ï¿½ï¿½Ê‚ï¿½,");
		sort2.show();
		System.out.println("ï¿½É‚È‚ï¿½ï¿½Ü‚ï¿½ï¿½ï¿½.");

		/* sort2ï¿½ï¿½bubbleLessï¿½ï¿½ï¿½Äo */
		sort2.bubbleLess();
		System.out.println("ï¿½~ï¿½ï¿½ï¿½Éƒoï¿½uï¿½ï¿½ï¿½\ï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Ê‚ï¿½,");
		sort2.show();
		System.out.println("ï¿½Å‚ï¿½.");

		/* ï¿½ï¿½ï¿½\ï¿½bï¿½hï¿½oï¿½Rï¿½ÅƒAï¿½Nï¿½Zï¿½X */
		System.out.println("ï¿½Å‘ï¿½ï¿½l:" + sort2.getFirst() + ", " +
					(r_max-r_min+1)/2 + "ï¿½Ô–Ú‚Ì’l:" + sort2.getInd((r_max-r_min+1)/2) +
					", ï¿½Åï¿½ï¿½l:" + sort2.getLast() + "ï¿½Å‚ï¿½.");

	}
}
