package pa;
class Car /*extends Vehicle*/{

	private int num;
	private double gas;


	public Car (){
		num = 0;
		gas = 0;
		System.out.println("車を作成しました");

	}
	public setCar(int n,int g){
		num = n;
		gas = g;
		System.out.println("ナンバー"+n+"ガソリン量"+g+"の車を作成しました");
	}

	public void show(){

		System.out.println("車のナンバーは"+num+"です。");
		System.out.println("車の ガソリン量は"+gas+"です。");
		//	System.out.println("車の速度は"+speed+"です。");
	}
}
