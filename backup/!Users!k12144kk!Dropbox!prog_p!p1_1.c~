#include<stdio.h>
int main (void){
  int a,b,c;
  printf("３つの整数値を入力してください");
  printf("整数値1:");scanf("%d",&a);
  printf("整数値2:");scanf("%d",&b);
  printf("整数値3:");scanf("%d",&c);
  
  if (a == b == c) printf("３つの数字は同じです\n");
  else if (a == b|| b== c||c ==a )printf("2つの数字が同じです\n");
  else printf("すべての数字が異なります\n");

  return 0;
}
